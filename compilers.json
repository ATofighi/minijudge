{
    "clang-c": {
        "extensions": [".c"],
        "options": "clang -O2 -Wall {0}",
        "executable_file": "./a.out"
    },
    "clang-c11": {
        "extensions": [".c"],
        "options": "clang -O2 -Wall -std=c11 {0}",
        "executable_file": "./a.out"
    },
    "clang-cpp": {
        "extensions": [".cpp", ".c++"],
        "options": "clang++ -O2 -Wall {0}",
        "executable_file": "./a.out"
    },
    "clang-cpp11": {
        "extensions": [".cpp", ".c++"],
        "options": "clang++ -O2 -Wall -std=c++11 {0}",
        "executable_file": "./a.out"
    },
    "fpc": {
        "extensions": [".pas"],
        "options": "fpc -O2 -vew {0}",
        "executable_file": "{1}"
    },
    "java": {
        "extensions": [".java"],
        "options": "javac {0}",
        "executable_file": "{1}.class",
        "runtime": "java -Xmx512M -Xss64M {1}"
    },
    "python3": {
        "extensions": [".py"],
        "runtime": "python3 {0}"
    }
}
